<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>run</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>javafx:run</goal>
        </goals>
    </action>
    <action>
        <actionName>debug</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>javafx:run@debug</goal>
        </goals>
        <properties>
            <jpda.listen>true</jpda.listen>
        </properties>
    </action>
    <action>
        <actionName>profile</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>javafx:run@profile</goal>
        </goals>
    </action>
    <action>
            <actionName>run.single.main</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>javafx:run</goal>
            </goals>
        </action>
    <action>
            <actionName>CUSTOM-Run Server</actionName>
            <displayName>Run Server</displayName>
            <goals>
                <goal>exec:java</goal>
                <goal>-Dexec.mainClass="com.mycompany.drs.server.DRSServer"</goal>
            </goals>
        </action>
</actions>
